{
  "name": "jamaid",
  "version": "0.7.1",
  "description": "Convert FigJam flow diagrams into Mermaid markdown",
  "type": "module",
  "bin": {
    "jamaid": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "figma",
    "figjam",
    "mermaid",
    "cli",
    "typescript"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "fast-xml-parser": "^4.5.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
